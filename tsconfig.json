{
    "compilerOptions": {
        "outDir": "./build",
        "rootDir": "./src",
        "noImplicitAny": false,
        "esModuleInterop": true,
        "target": "es5",
        "moduleResolution": "node",
        "mapRoot": "",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "strict": false,
        "sourceMap": true,
        "allowJs": true,
        "lib": ["es2015", "dom"]
    }
}
